<div class="container container-fluid m-5">
    

    @{
        ViewBag.Title = "Restaurar Contraseña";
    }

    
    
    <div class="row">
        
        <div class="col-md-4"></div>
        <div class="col-md-4">
            <h2 class="text-center h3">@ViewBag.Title.</h2>
            <hr />
            <form>
                <div class="form-group">
                    <label for="exampleInputEmail1">Nueva Contraseña</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
                    <small id="emailHelp" class="form-text text-muted">Contraseñas no coinciden..!</small>
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Repetir Contraseña</label>
                    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
                </div>

                <button type="submit" class="btn btn-outline-secondary">cancelar</button>
                <button type="submit" class="btn btn-outline-secondary">Aceptar</button>
            </form>
        </div>
        <div class="col-md-4"></div>
    </div>
</div>